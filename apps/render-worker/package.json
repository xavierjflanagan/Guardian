{
  "name": "exora-v3-worker",
  "version": "1.0.0",
  "description": "V3 Background Job Processing Worker - Source of Truth",
  "main": "dist/worker.js",
  "scripts": {
    "build": "tsc --project ./tsconfig.json",
    "start": "node --expose-gc --max-old-space-size=450 dist/worker.js",
    "dev": "tsx src/worker.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "generate-sapbert": "tsx src/pass15/generate-sapbert-embeddings.ts",
    "deploy:staging": "cp -r ./* ../../../../../../../apps/render-worker/ && cd ../../../../../../../ && git add apps/render-worker && git commit -m 'Deploy worker to staging' && git push origin staging",
    "deploy:production": "cp -r ./* ../../../../../../../apps/render-worker/ && cd ../../../../../../../ && git add apps/render-worker && git commit -m 'Deploy worker to production' && git push origin main"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@supabase/supabase-js": "^2.38.0",
    "@types/heic-convert": "^2.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "heic-convert": "^2.1.0",
    "node-poppler": "^8.0.4",
    "openai": "^4.20.1",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.19",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.19.11",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.5",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "healthcare",
    "background-processing",
    "v3-architecture",
    "job-queue"
  ],
  "author": "Exora Health Pty Ltd",
  "license": "UNLICENSED",
  "private": true,
  "config": {
    "loglevel": "verbose"
  }
}
