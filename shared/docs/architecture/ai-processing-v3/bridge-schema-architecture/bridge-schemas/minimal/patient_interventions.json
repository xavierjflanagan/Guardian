{
  "table_name": "patient_interventions",
  "description": "Medical interventions: medications, procedures, treatments",
  
  "required_fields": {
    "intervention_type": {
      "enum": ["medication", "procedure", "therapy", "surgery"]
    },
    
    "intervention_name": "string",
    
    "intervention_date": "ISO 8601",
    
    "confidence_score": "number (0.0-1.0)"
  },
  
  "optional_fields": {
    "dosage": "string",
    "frequency": "string",
    "route": "string",
    "duration": "string",
    "indication": "string",
    "performed_by": "string",
    "outcome": "string",
    "quantity": "string",
    "snomed_code": "string",
    "cpt_code": "string",
    "ndc_code": "string",
    "coding_confidence": "number (0.0-1.0)"
  },
  
  "validation": {
    "confidence_threshold": 0.7
  },
  
  "output_example": {
    "intervention_type": "medication",
    "intervention_name": "Lisinopril 10mg",
    "dosage": "10mg",
    "frequency": "once daily",
    "indication": "hypertension",
    "intervention_date": "2024-07-15T14:30:00Z",
    "confidence_score": 0.92
  }
}