{
  "table": "patient_immunizations",
  "pass": "pass-2",
  "required": ["patient_id", "event_id", "vaccine_name", "administration_date"],
  "fields": {
    "patient_id": "uuid (context, has patient_id column)",
    "event_id": "uuid (parent clinical event, NOT NULL)",
    "vaccine_name": "string (NOT NULL)",
    "administration_date": "timestamptz (NOT NULL, ISO 8601 with timezone)",
    "vaccine_type": "string? (mRNA, inactivated, live attenuated, subunit)",
    "manufacturer": "string?",
    "lot_number": "string?",
    "expiration_date": "date?",
    "dose_number": "integer?",
    "dose_amount": "numeric(6,3)? (mL)",
    "route_of_administration": "string? (intramuscular, intranasal, oral, subcutaneous)",
    "anatomical_site": "string? (left deltoid, right deltoid, etc.)",
    "snomed_code": "string?",
    "cpt_code": "string?",
    "cvx_code": "string? (CDC vaccine codes)",
    "ndc_code": "string?",
    "acir_code": "string? (Australian)",
    "pbs_item_code": "string? (Australian)",
    "indication": "string?",
    "contraindications": "string[]? (TEXT[] array)",
    "adverse_reactions": "string[]? (TEXT[] array, triggers review)",
    "administered_by": "string?",
    "administering_facility": "string?",
    "coding_confidence": "numeric(4,3)? 0.000-1.000",
    "source_shell_file_id": "uuid?",
    "ai_extracted": "boolean (true)",
    "ai_confidence": "numeric(4,3) 0.000-1.000",
    "requires_review": "boolean (true if adverse_reactions)",
    "clinical_validation_status": "pending|validated|requires_review|rejected? (default: pending)"
  },
  "vaccine_types": {
    "mRNA": "COVID-19 Pfizer, Moderna",
