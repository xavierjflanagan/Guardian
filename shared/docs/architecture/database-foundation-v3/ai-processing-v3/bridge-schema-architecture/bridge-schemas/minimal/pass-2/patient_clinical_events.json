{
  "table": "patient_clinical_events",
  "pass": "pass-2",
  "required": [
    "patient_id",
    "shell_file_id",
    "activity_type",
    "clinical_purposes",
    "event_name",
    "event_date"
  ],
  "fields": {
    "patient_id": "uuid (context)",
    "shell_file_id": "uuid (context)",
    "activity_type": "observation|intervention",
    "clinical_purposes": "array[screening|diagnostic|therapeutic|monitoring|preventive]",
    "event_name": "string",
    "event_date": "timestamptz",
    "encounter_id": "uuid?",
    "method": "string?",
    "body_site": "string?",
    "performed_by": "string?",
    "facility_name": "string?",
    "snomed_code": "string?",
    "loinc_code": "string?",
    "cpt_code": "string?",
    "icd10_code": "string?",
    "ai_extracted": "boolean (true)",
    "ai_confidence": "numeric(0.000-1.000)",
    "ai_model_version": "string (v3)",
    "entity_id": "string?",
    "requires_review": "boolean",
    "ai_processing_version": "string (v3)",
    "entity_extraction_completed": "boolean",
    "clinical_data_extracted": "boolean",
    "requires_manual_review": "boolean",
    "ai_confidence_scores": "jsonb?",
    "coding_confidence": "numeric(0.000-1.000)?",
    "coding_method": "automated_ai|manual_verification|hybrid_validation?",
    "contains_phi": "boolean (true)"
  },
  "o3_classification": {
    "activity_type": {
      "observation": "measurements, tests, assessments",
      "intervention": "treatments, medications, procedures"
    },
    "clinical_purposes": {
      "screening": "routine checks",
      "diagnostic": "identify conditions",
      "therapeutic": "treat disease",
      "monitoring": "track progress",
      "preventive": "prevent disease"
    }
  },
  "example": {
    "activity_type": "observation",
    "clinical_purposes": ["monitoring"],
    "event_name": "Blood Pressure Measurement",
    "event_date": "2025-09-30T14:30:00Z",
    "ai_extracted": true,
    "ai_confidence": 0.960,
    "ai_model_version": "v3",
    "entity_extraction_completed": true,
    "clinical_data_extracted": true,
    "requires_review": false,
    "requires_manual_review": false,
    "ai_processing_version": "v3"
  }
}