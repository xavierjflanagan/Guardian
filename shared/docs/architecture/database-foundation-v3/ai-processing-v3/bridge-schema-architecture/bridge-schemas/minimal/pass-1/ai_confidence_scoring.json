{
  "table": "ai_confidence_scoring",
  "pass": "pass-1",
  "operation": "CREATE",
  "required": ["processing_session_id"],
  "fields": {
    "processing_session_id": "uuid (context)",
    "entity_processing_audit_id": "uuid? (context, if entity-level)",
    "entity_detection_confidence": "numeric(4,3)? 0.000-1.000",
    "text_extraction_confidence": "numeric(4,3)? 0.000-1.000",
    "spatial_alignment_confidence": "numeric(4,3)? 0.000-1.000",
    "vision_model_confidence": "numeric(4,3)? 0.000-1.000",
    "language_model_confidence": "numeric(4,3)? 0.000-1.000",
    "classification_model_confidence": "numeric(4,3)? 0.000-1.000",
    "overall_confidence": "numeric(4,3)? 0.000-1.000 (weighted avg)",
    "reliability_score": "numeric(4,3)? 0.000-1.000",
    "clinical_relevance_score": "numeric(4,3)? 0.000-1.000",
    "confidence_trend": "improving|stable|declining?",
    "outlier_detection": "boolean? (default: false)",
    "confidence_flags": "text[]? (quality issues)",
    "human_validation_available": "boolean? (default: false)",
    "human_agreement_score": "numeric(4,3)? 0.000-1.000",
    "model_accuracy_score": "numeric(4,3)? 0.000-1.000",
    "processing_time_ms": "integer?",
    "model_version": "string? (e.g., gpt-4o-mini-2024-07-18)",
    "calibration_score": "numeric(4,3)? 0.000-1.000"
  },
  "pass2_may_add": ["clinical_coding_confidence"],
  "example": {
