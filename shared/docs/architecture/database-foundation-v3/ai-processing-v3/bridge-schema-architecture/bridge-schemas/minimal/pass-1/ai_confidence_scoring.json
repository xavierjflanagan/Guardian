    "reliability_score": 0.900,
    "clinical_relevance_score": 0.870,
    "confidence_trend": "stable",
    "outlier_detection": false,
    "processing_time_ms": 8450,
    "model_version": "gpt-4o-mini-2024-07-18",
    "calibration_score": 0.910
  }
}
