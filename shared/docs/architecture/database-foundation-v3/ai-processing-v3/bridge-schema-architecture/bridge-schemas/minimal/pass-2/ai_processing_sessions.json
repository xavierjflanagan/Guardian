{
  "table": "ai_processing_sessions",
  "pass": "pass-2",
  "operation": "UPDATE",
  "fields": {
    "session_status": "completed|failed?",
    "workflow_step": "clinical_extraction|completed?",
    "completed_steps": "integer? (set to total_steps)",
    "overall_confidence": "numeric(4,3)? (use COALESCE)",
    "quality_score": "numeric(4,3)?",
    "processing_completed_at": "timestamptz?",
    "total_processing_time": "interval? (calculate from start)",
    "error_message": "string? (if failed)",
    "error_context": "jsonb? (include pass=pass-2)",
    "retry_count": "integer? (increment)"
  },
  "readonly": ["patient_id", "shell_file_id", "session_type", "processing_started_at"],
  "example": {
    "session_status": "completed",
    "workflow_step": "completed",
    "completed_steps": 5,
    "processing_completed_at": "2025-01-01T12:36:22Z",
    "total_processing_time": "00:01:26"
  }
}
